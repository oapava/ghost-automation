{
  "id": "backstop_default",
  "viewports": [
    {
      "label": "desktop",
      "width": 1280,
      "height": 800
    }
  ],
  "scenarios": [
    {
      "label": "Dummy scenario",
      "url": "about:blank",
      "selectors": ["document"],
      "misMatchThreshold": 0.1
    }
  ],
  "engine": "puppeteer",
  "engineOptions": {
    "args": ["--no-sandbox"]
  },
  "paths": {
    "bitmaps_reference": "backstop_data/bitmaps_reference",
    "bitmaps_test": "backstop_data/bitmaps_test",
    "html_report": "backstop_data/html_report",
    "ci_report": "backstop_data/ci_report"
  },
  "report": ["browser", "CI"],
  "ci": {
    "disable": true
  },
  "misMatchThreshold": 0.1
}